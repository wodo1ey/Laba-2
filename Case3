#include "iostream"
using namespace std;

void main() {
	setlocale(LC_ALL, "Russian");
	int x;
	cin >> x;
	switch (x) {
	case 1: case 2: case 12: cout << "зима"; break;
	case 3: case 4: case 5: cout << "весна"; break;
	case 6: case 7: case 8: cout << "лето"; break;
	case 9: case 10: case 11: cout << "осень"; break;
	}
}
